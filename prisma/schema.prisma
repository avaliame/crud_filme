generator client {
  provider        = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Genero {
  id        Int     @id @default(autoincrement())
  nome      String   @unique
  filmes    Filme[] Â  
}

model Filme {
  id        Int     @id @default(autoincrement())
  titulo    String
  ano       Int
  Datalancamento DateTime
  genero    Genero   @relation(fields: [generoId], references: [id])
  generoId  Int
  diretor   String
}